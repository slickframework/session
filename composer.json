  "description": "Session handling for Slick Framework",
  "minimum-stability": "dev",
  "license": "MIT",
  "version": "v1.2.0",
  "keywords": [
    "slick",
    "session",
    "session handling"
  ],
  "authors": [
    {
      "name": "Filipe Silva",
      "email": "silvam.filipe@gmail.com",
